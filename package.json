{
  "name": "create-fetcher",
  "version": "0.2.1",
  "description": "customizable data fetcher wrapper for remote data fetching",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "del-cli lib",
    "build": "npm run clean && npm run compile",
    "compile": "tsc --outDir lib -d",
    "start": "npm run compile -- --watch",
    "release": "npm test && npm run build && standard-version && git push --follow-tags origin master && npm publish",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocwind/create-fetcher.git"
  },
  "keywords": [
    "fetch",
    "swr",
    "request"
  ],
  "author": "Roc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rocwind/create-fetcher/issues"
  },
  "homepage": "https://github.com/rocwind/create-fetcher#readme",
  "devDependencies": {
    "@react-native-community/async-storage": "1.10.0",
    "@types/jest": "25.2.1",
    "@types/object-hash": "1.3.1",
    "@types/react": "16.9.34",
    "del-cli": "3.0.0",
    "jest": "26.0.1",
    "react": "16.13.1",
    "standard-version": "8.0.0",
    "ts-jest": "25.5.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "object-hash": "^2.0.3"
  }
}
